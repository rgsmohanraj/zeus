    <subDataset name="transaction" uuid="f13e6d36-5148-4ecc-bbe3-3035def80980">
        <parameter name="asOnDate" class="java.lang.String"/>
        <parameter name="accountNo" class="java.lang.String"/>
        <queryString>
            <![CDATA[select date_format(soa.valuedate, "%d-%m-%Y") as "Date", particulars as "Particulars", description as "Description",  debit as "Debit", credit as "Credit", balance as "Balance" from (

select date_format(txn.createdon_date, "%d-%m-%Y") as 'transactiondate', txn.value_date AS 'valuedate', 'Fee/Charge' as 'particulars',c.fees_type as 'description', ROUND((lc.amount + lc.total_gst),2) AS 'debit', '' as 'credit', '' as 'balance', '1' as 'order'

from m_loan_charge lc inner join m_loan l on lc.loan_id = l.id inner join m_loan_transaction txn on l.id = txn.loan_id inner join m_charge c on lc.charge_id = c.id where lc.loan_id = (select id from m_loan where account_no =$P{accountNo} )

and lc.charge_time_enum = 1 and txn.transaction_type_enum = 5

union

select date_format(createdon_date , "%d-%m-%Y") as 'transactiondate', value_date as 'valuedate','Loan' as 'particulars' ,'Net Disbursement Amount' as 'description', '' as 'debit', ROUND((select net_disbursal_amount from m_loan where account_no =$P{accountNo}),2) as 'credit',ROUND(outstanding_loan_balance_derived,2) as 'balance', '2' as 'order'

from m_loan_transaction where loan_id = (select id from m_loan where account_no =$P{accountNo}) and transaction_type_enum = 1

union

select date_format(createdon_date , "%d-%m-%Y") as 'transactiondate', value_date as 'valuedate','Repayment' as 'particulars' ,'Principal Payment' as 'description', ROUND(principal_portion_derived,2) as 'debit', '' as 'credit',

ROUND(outstanding_loan_balance_derived,2) as 'balance', '5' as 'order' from m_loan_transaction where loan_id = (select id from m_loan where account_no =$P{accountNo}) and transaction_type_enum = 2 and is_reversed =0 and event not in ('PROCESSEDADVANCE','UNPROCESSEDADVANCE') and    principal_portion_derived !=0

union

select date_format(createdon_date , "%d-%m-%Y") as 'transactiondate', value_date as 'valuedate','Repayment' as 'particulars','Interest Payment' as 'description', ROUND(interest_portion_derived,2) as 'debit', '' as 'credit',

'' as 'balance', '4' as 'order' from m_loan_transaction where loan_id = (select id from m_loan where account_no =$P{accountNo}) and transaction_type_enum = 2 and is_reversed =0 and event not in ('PROCESSEDADVANCE','UNPROCESSEDADVANCE') and  interest_portion_derived !=0

union

select date_format(createdon_date , "%d-%m-%Y") as 'transactiondate', value_date as 'valuedate','Repayment' as 'particulars','Advance' as 'description', ROUND(advance_amount,2) as 'debit', '' as 'credit','' as 'balance', '3' as 'order'

from m_loan_transaction where loan_id = (select id from m_loan where account_no =$P{accountNo}) and transaction_type_enum = 2 and event in ('UNPROCESSEDADVANCE') and advance_amount != 0

union

select date_format(createdon_date , "%d-%m-%Y") as 'transactiondate', value_date as 'valuedate','Repayment' as 'particulars','Principal Payment' as 'description', ROUND(principal_portion_derived,2) as 'debit', '' as 'credit',ROUND(outstanding_loan_balance_derived,2) as 'balance', '5' as 'order'

from m_loan_transaction where loan_id = (select id from m_loan where account_no =$P{accountNo}) and transaction_type_enum = 2 and is_reversed =0   and principal_portion_derived != 0

union

select date_format(createdon_date , "%d-%m-%Y") as 'transactiondate', value_date as 'valuedate','Repayment' as 'particulars','Interest Payment' as 'description', ROUND(interest_portion_derived,2) as 'debit', '' as 'credit','' as 'balance', '4' as 'order'

from m_loan_transaction where loan_id = (select id from m_loan where account_no =$P{accountNo}) and transaction_type_enum = 2 and is_reversed =0 and interest_portion_derived !=0

union

select date_format(txn.createdon_date, "%d-%m-%Y") as 'transactiondate',
txn.value_date AS 'valuedate', 'Bounce Charge' as 'particulars','Bounce Charge' as 'description',
ROUND((lc.amount + lc.total_gst),2) AS 'debit', '' as 'credit', '' as 'balance', '6' as 'order' from m_loan l
left join m_loan_charge lc on lc.loan_id = l.id
left join m_loan_charge_paid_by lcp on lcp.loan_charge_id = lc.id
left join m_loan_transaction txn on txn.id = lcp.loan_transaction_id
where lc.loan_id = (select id from m_loan where account_no =$P{accountNo} )
and lc.charge_time_enum = 19 and lc.is_active = 1 and lc.amount_paid_derived > 0

union

    select date_format(txn.createdon_date, "%d-%m-%Y") as 'transactiondate', txn.value_date AS 'valuedate', 'Bounce Charges Waived ' as 'particulars','Bounce Charge Waived' as 'description', ROUND(lc.amount_waived_derived,2) AS 'debit', '' as 'credit', '' as 'balance', '7' as 'order'

from m_loan_charge lc inner join m_loan l on lc.loan_id = l.id inner join m_loan_transaction txn on l.id = txn.loan_id inner join m_charge c on lc.charge_id = c.id where lc.loan_id = (select id from m_loan where account_no =$P{accountNo} )

    and txn.transaction_type_enum = 9 and amount_waived_derived !=0

	union

select date_format(createdon_date , "%d-%m-%Y") as 'transactiondate', value_date as 'valuedate','Foreclosure' as 'particulars','Foreclosure Principal Collection' as 'description', ROUND(principal_portion_derived,2) as 'debit', '' as 'credit',ROUND(outstanding_loan_balance_derived,2) as 'balance', '10' as 'order'

from m_loan_transaction where loan_id = (select id from m_loan where account_no =$P{accountNo}) and transaction_type_enum = 11

union

select date_format(createdon_date , "%d-%m-%Y") as 'transactiondate', value_date as 'valuedate','Foreclosure' as 'particulars','Foreclosure Interest Collection' as 'description', ROUND(interest_portion_derived,2) as 'debit', '' as 'credit','' as 'balance', '9' as 'order'

from m_loan_transaction where loan_id = (select id from m_loan where account_no =$P{accountNo} ) and transaction_type_enum = 11

union

select date_format(createdon_date , "%d-%m-%Y") as 'transactiondate', value_date as 'valuedate','Foreclosure Charge' as 'particulars','Foreclosure Charge Collection' as 'description', ROUND(fee_charges_portion_derived,2) as 'debit', '' as 'credit','' as 'balance', '8' as 'order'

from m_loan_transaction where loan_id = (select id from m_loan where account_no = $P{accountNo} ) and transaction_type_enum = 11 and (fee_charges_portion_derived <> 0 or fee_charges_portion_derived <> null)
) as soa
where soa.valuedate <= STR_TO_DATE(($P{asOnDate}), "%d %M %Y")
order by soa.valuedate,soa.order,soa.particulars,soa.description,soa.balance desc]]>
        </queryString>
