{
  "name": "zeus-web-app",
  "version": "0.0.0",
  "description": "MifosX Web App is the default web application built on top of the Fineract platform for the Mifos user community leveraging the popular Angular framework.",
  "keywords": [
    "mifos",
    "openmf",
    "fineract",
    "angular"
  ],
  "homepage": "https://github.com/openMF/web-app",
  "license": "MPL-2.0",
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod --output-hashing=none",
    "build:prod": "node --max-old-space-size=16384 ./node_modules/@angular/cli/bin/ng build --prod --output-hashing=none --base-href=/web-app/",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "node version.js",
    "postinstall": "node version.js && ngcc"
  },
  "dependencies": {
    "@angular-devkit/schematics": "^15.2.10",
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/localize": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@angular/service-worker": "^15.2.10",
    "@babel/traverse": "^7.23.2",
    "@ckeditor/ckeditor5-angular": "1.2.3",
    "@ckeditor/ckeditor5-build-classic": "21.0.0",
    "@fortawesome/angular-fontawesome": "^0.10.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "5.8.2",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bn-ng-idle": "^2.0.5",
    "chart.js": "3.0.0-alpha",
    "core-js": "2.5.0",
    "crypto-js": "^4.2.0",
    "css-fx-layout": "^3.0.4",
    "font-awesome": "^4.7.0",
    "install": "^0.13.0",
    "loader-utils": "^2.0.3",
    "lodash": "4.17.21",
    "minimatch": "^3.0.5",
    "moment": "^2.29.4",
    "ngx-spinner": "^15.0.1",
    "ngx-toastr": "^15.2.2",
    "rxjs": "6.6.2",
    "tslib": "^2.0.0",
    "xlsx": "^0.18.5",
    "zone.js": "0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/language-service": "^15.2.10",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ngx-rocket/scripts": "3.0.3",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash": "4.14.132",
    "@types/node": "12.11.1",
    "codelyzer": "6.0.0",
    "git-describe": "^4.1.1",
    "hads": "^3.0.3",
    "htmlhint": "0.11.0",
    "https-proxy-agent": "^3.0.0",
    "jasmine-core": "^4.2.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-junit-reporter": "1.2.0",
    "protractor": "~7.0.0",
    "puppeteer": "^15.3.2",
    "stylelint": "10.0.1",
    "stylelint-config-recommended-scss": "3.3.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-scss": "3.6.1",
    "ts-node": "8.2.0",
    "tslint": "~6.1.0",
    "typescript": "4.9.5"
  },
  "publishConfig": {
    "registry": "https://vcpl.jfrog.io/artifactory/api/npm/vcpl-npm-app-npm-remote/"
  },
  "engines": {
    "node": ">= 16.15.1",
    "npm": ">= 8.11.0"
  }
}
