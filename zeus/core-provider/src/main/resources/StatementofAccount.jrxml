    <parameter name="asOnDate" class="java.lang.String"/>
    <parameter name="accountNo" class="java.lang.String"/>
    <queryString>
        <![CDATA[select cl.display_name as "Customer Name",
cl.mobile_no as "Mobile",
cl.email_address as "Email id",
(select date_format((STR_TO_DATE(($P{asOnDate}),"%d %M %Y")),"%d-%m-%Y")) as "Date",
lh.account_no as "Loan Account Number",
cvp.code_value as "Loan Type/Product",
pr.partner_name as "Colending Partner Name",
Round(lh.approved_principal,2) as "Sanction Amount",
date_format(lh.disbursedon_date,"%d-%m-%Y") as "Date of Disbursement",
concat(Round(lo.nominal_interest_rate_per_period,2),"%") as "Rate of Interest(ROI)",
(CASE
	when (lh.loan_status_id = 710 AND lo.loan_status_id = 710) THEN (
    (SELECT date_format(duedate,"%d-%m-%Y") FROM m_loan_repayment_schedule
		WHERE loan_id = lo.id and date_format(duedate,"%Y-%m-%d") <= STR_TO_DATE(($P{asOnDate}) , "%d %M %Y")
		and (obligations_met_on_date is null or date_format(obligations_met_on_date,"%Y-%m-%d") > STR_TO_DATE(($P{asOnDate}) , "%d %M %Y")) order by installment LIMIT 1)
	UNION
	(SELECT date_format(duedate,"%d-%m-%Y") FROM m_loan_repayment_schedule
		WHERE loan_id = lo.id and duedate > STR_TO_DATE(($P{asOnDate}) , "%d %M %Y") order by installment LIMIT 1)LIMIT 1)
	when (lh.loan_status_id = 300 AND lo.loan_status_id = 710) THEN (
    (SELECT date_format(duedate,"%d-%m-%Y") FROM m_loan_repayment_schedule_history
		WHERE loan_id = lo.id and date_format(duedate,"%Y-%m-%d") <= STR_TO_DATE(($P{asOnDate}) , "%d %M %Y")
		and (obligations_met_on_date is null or date_format(obligations_met_on_date,"%Y-%m-%d") > STR_TO_DATE(($P{asOnDate}) , "%d %M %Y")) order by installment LIMIT 1)
	UNION
	(SELECT date_format(duedate,"%d-%m-%Y") FROM m_loan_repayment_schedule_history
		WHERE loan_id = lo.id and duedate > STR_TO_DATE(($P{asOnDate}) , "%d %M %Y") order by installment LIMIT 1)LIMIT 1)
	when (lh.loan_status_id = 300 || lh.loan_status_id = 600) THEN (
	(SELECT date_format(duedate,"%d-%m-%Y") FROM m_loan_repayment_schedule
		WHERE loan_id = lo.id and date_format(duedate,"%Y-%m-%d") <= STR_TO_DATE(($P{asOnDate}) , "%d %M %Y")
		and (obligations_met_on_date is null or date_format(obligations_met_on_date,"%Y-%m-%d") > STR_TO_DATE(($P{asOnDate}) , "%d %M %Y")) order by installment LIMIT 1)
	UNION
	(SELECT date_format(duedate,"%d-%m-%Y") FROM m_loan_repayment_schedule
		WHERE loan_id = lo.id and duedate > STR_TO_DATE(($P{asOnDate}) , "%d %M %Y") order by installment LIMIT 1)LIMIT 1)
 END) as "Due Date",

 Round(lh.principal_outstanding_derived,2) as "Principal Outstanding",
Round(lh.principal_overdue_derived + lh.interest_overdue_derived,2) as "Overdue Amount",
(CASE
	when lo.loan_status_id = 710
		THEN IF(date_format(lo.closedon_date,"%Y-%m-%d") <= STR_TO_DATE(($P{asOnDate}) , "%d %M %Y"),  0,
		((SELECT count(*) FROM m_loan_repayment_schedule_history WHERE loan_id = lo.id AND (duedate = STR_TO_DATE(($P{asOnDate}) , "%d %M %Y") AND obligations_met_on_date IS NULL)) +
		(SELECT count(*) FROM m_loan_repayment_schedule_history WHERE loan_id = lo.id AND (duedate < STR_TO_DATE(($P{asOnDate}) , "%d %M %Y") AND obligations_met_on_date IS NULL)) +
		(SELECT count(*) FROM m_loan_repayment_schedule_history WHERE loan_id = lo.id AND (duedate > STR_TO_DATE(($P{asOnDate}) , "%d %M %Y")
			OR (obligations_met_on_date < STR_TO_DATE(($P{asOnDate}) , "%d %M %Y") AND obligations_met_on_date IS NULL)
			OR (obligations_met_on_date > STR_TO_DATE(($P{asOnDate}) , "%d %M %Y"))))))
	when lo.loan_status_id = 300
		THEN ((SELECT count(*) FROM m_loan_repayment_schedule WHERE loan_id = lo.id AND (duedate = STR_TO_DATE(($P{asOnDate}) , "%d %M %Y") AND obligations_met_on_date IS NULL)) +
			  (SELECT count(*) FROM m_loan_repayment_schedule WHERE loan_id = lo.id AND (duedate < STR_TO_DATE(($P{asOnDate}) , "%d %M %Y") AND obligations_met_on_date IS NULL)) +
	          (SELECT count(*) FROM m_loan_repayment_schedule WHERE loan_id = lo.id AND (duedate > STR_TO_DATE(($P{asOnDate}) , "%d %M %Y")
				OR (obligations_met_on_date < STR_TO_DATE(($P{asOnDate}) , "%d %M %Y") AND obligations_met_on_date IS NULL)
				OR (obligations_met_on_date > STR_TO_DATE(($P{asOnDate}) , "%d %M %Y")))))
	when lo.loan_status_id = 600 THEN IF(date_format(lo.closedon_date,"%Y-%m-%d") <= STR_TO_DATE(($P{asOnDate}) , "%d %M %Y"),  0,
		((SELECT count(*) FROM m_loan_repayment_schedule WHERE loan_id = lo.id AND (duedate = STR_TO_DATE(($P{asOnDate}) , "%d %M %Y") AND obligations_met_on_date IS NULL)) +
		(SELECT count(*) FROM m_loan_repayment_schedule WHERE loan_id = lo.id AND (duedate < STR_TO_DATE(($P{asOnDate}) , "%d %M %Y") AND obligations_met_on_date IS NULL)) +
		(SELECT count(*) FROM m_loan_repayment_schedule WHERE loan_id = lo.id AND (duedate > STR_TO_DATE(($P{asOnDate}) , "%d %M %Y")
			OR (obligations_met_on_date < STR_TO_DATE(($P{asOnDate}) , "%d %M %Y") AND obligations_met_on_date IS NULL)
			OR (obligations_met_on_date > STR_TO_DATE(($P{asOnDate}) , "%d %M %Y"))))))
 END) as  "Pending Tenure"
 from m_loan_history lh
 left join m_product_loan lp on lp.id = lh.product_id
 left join m_loan_repayment_schedule rs on rs.loan_id = lh.loan_id
 left join m_loan_repayment_schedule_history rsh on rsh.loan_id = lh.loan_id
 left join m_loan lo on lo.id = lh.loan_id
 left join m_partner pr on pr.id = lh.partner_id
 left join m_loan_transaction txn on txn.loan_id = lh.loan_id
 left join m_code_value cvp on cvp.id = lp.asset_class_cv_id
 left join m_client cl on cl.id = lh.client_id
 where lh.rev = (Select max(rev) from m_loan_history lh where lh.loan_id = lo.id and date_format(lh.value_date,'%Y-%m-%d') <= STR_TO_DATE(($P{asOnDate}), "%d %M %Y"))
 and lh.account_no = $P{accountNo} group by lh.id]]>
    </queryString>
